<Window x:Class="AistTrader.WhatsNew"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Name="WhatsNewWindow"
        FontFamily="Consolas"
        Title="Аист Трейдер - Что нового"  
        ResizeMode="NoResize"
        mc:Ignorable="d"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Height="562.207"
        Width="466"
        Icon="Resources/Images/mainIcon.ico"
        WindowStartupLocation="CenterScreen"
        >
    <Grid Margin="1,1,1,6">
        <StackPanel>
            <!--Динамически подтягивать имя сборки-->
            <Expander Margin="5" IsExpanded="True" Padding="5" Header="Version..взять из номера сборки" >
                <TextBlock TextWrapping="Wrap" Height="169">
                        <!--*Настроено взаимодействие форм
                        <LineBreak/>
                        *Детальное указание того, что было сделано
                        <LineBreak/>
                        *Второе детальное указание-->
                </TextBlock>
            </Expander>
        </StackPanel>
    </Grid>
</Window>
