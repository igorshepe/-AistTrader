<Window x:Class="AistTrader.StrategiesSettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" x:Name="StratSettings"
        
        Title="Настройки стратегии" Height="316" Width="258" ResizeMode="NoResize" Icon="Resources/Images/mainIcon.ico">
    <Grid>
        <Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="16,232,0,0" VerticalAlignment="Top" Width="75" Click="OkButtonClick" IsDefault="True" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="143,232,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" />
        <xctk:PropertyGrid SelectedObject="{Binding ElementName=StratSettings, Path=Settings}" Margin="0,0,0,82" ShowSummary="False" ShowSearchBox="False" ShowSortOptions="False" UseLayoutRounding="True" />
    </Grid>
</Window>
